    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.5",
    "body-scroll-lock": "2.6.4",
    "css-loader": "3.2.0",
    "eslint": "6.3.0",
